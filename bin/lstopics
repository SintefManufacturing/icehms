#!/usr/bin/env python
import icehms
if __name__ == "__main__":
    mgr = icehms.IceManager()
    try:
        mgr.init()
        topics = mgr.topicMgr.retrieveAll()
        print("\nTopics are: \n")
        for name, prx in topics.items():
            print(name)
        topics = mgr.eventMgr.retrieveAll()
        print("\nEvents Topics are: \n")
        for name, prx in topics.items():
            print(name)
    finally:
        mgr.shutdown()

